{
  "$schema": "./node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
  "input": "../src/main/resources/swagger/api-v3.yaml",
  "output": "./src/app/api/web-console",
  "module": "WebConsoleApiModule",
  "configuration": "WebConsoleApiConfiguration",
  "defaultTag": "WebConsoleApi",
  "prefix": "WebConsoleApi",
  "ignoreUnusedModels": true,
  "minParamsForContainer": 2,
  "sortParams": "desc",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "enumModule": true,
  "generateExamples": false,
  "enumStyle": "upper"
}
